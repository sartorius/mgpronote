<script>
function validateMGSCode(t){return/^M[A-Z0-9]{8}$/.test(t)}function decodeMGSCodePartSecure(t){let n=parseInt(t.substring(1),36);return parseInt(n.toString().substring(n.toString().length-4))}function decodeMGSCodePartId(t){let n=parseInt(t.substring(1),36);return parseInt(n.toString().substring(0,n.toString().length-4))}function mgspad(t,n,e){return e=e||"0",(t+="").length>=n?t:new Array(n-t.length+1).join(e)+t}function mgsEncode(t,n){let e=parseInt(t.toString()+mgspad(n,4).toString());return"M"+mgspad(e.toString(36),8).toUpperCase()}
</script>
